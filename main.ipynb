   "source": [
    "def cost_function(frequency, stop_time, line_data,line_info,line_name, stop_time_w, unserved_cost_w, density_cost_w, standing_cost_w):\n",
    "    op_cost = 0\n",
    "\n",
    "    for date in line_data:\n",
    "        for hour in line_data[date]:\n",
    "            op_cost += frequency * line_info[line_name][\"energy_cost\"]\n",
    "            for station in line_data[date][hour]:\n",
    "\n",
    "                wait_cost = station[\"in_passenger\"] * frequency/2 + stop_time + station[\"cum_passenger_count\"] * stop_time_w\n",
    "                unserved_cost = max(0,(station[\"in_passenger\"] - (line_info[line_name][\"total_capacity\"] - station[\"cum_passenger_count\"]))) * unserved_cost_w \n",
    "                density_cost = station[\"cum_passenger_count\"] / line_info[line_name][\"total_capacity\"] * density_cost_w \n",
    "                standing_cost = (station[\"cum_passenger_count\"] - line_info[line_name][\"total_capacity\"]) * standing_cost_w\n",
    "\n",
    "    return op_cost + wait_cost + unserved_cost + density_cost + standing_cost\n"
   ]
  },
