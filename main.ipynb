   "metadata": {},
   "outputs": [],
   "source": [
    "def select_population(population, fitnesses):\n",
    "    sorted_pop = [x for _, x in sorted(zip(fitnesses, population))]\n",
    "    return sorted_pop[:POPULATION_SIZE // 2]\n"
   ]
  },
  {
   "cell_type": "code",

   "id": "5226a476",
   "metadata": {},
   "outputs": [],
   "source": [
    "def crossover(parents):\n",
    "    offspring = []\n",
    "    while len(offspring) < POPULATION_SIZE:\n",
    "        p1, p2 = random.sample(parents, 2)\n",
    "        point = random.randint(1, NUM_LINES - 1)\n",
    "        new_freq = p1[0][:point] + p2[0][point:]\n",
    "        new_stop = p1[1][:point] + p2[1][point:]\n",
    "        offspring.append((new_freq, new_stop))\n",
    "    return offspring"
   ]
  },
